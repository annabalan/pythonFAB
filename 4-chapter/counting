# Counting

print("\t\t'Welcome to Your Counter!")

start = int(input("Enter a number to start with: "))
end = int(input("Enter a number to end with: "))
by = int(input("Enter the amount by which to count: "))

for i in range(start, end, by):
  print(i, end=" ")

input("\n\nPress the enter key to exit.\n")
