nested = ("second", "third", ["fourth", "fifth", "sixth"])
print(nested)

scores = [("Moe", 1000), ("Larry", 1500), ("Curly", 3000)]
print(scores)
print(scores[1])
print(scores[2])

a_score = scores[2]
print(a_score)
print(a_score[0])

print(scores[2][0])

name, score = ("Shemp", 175)
print(name)
print(score)
