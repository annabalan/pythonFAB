# Attribute Critter
# Demonstrates creating and accessing object attributes

class Critter(object):
  """A virtual pet"""
  def_init_(self, name):
    print("A new critter has been born!")
    self.name = name
  def_str_(self):
      rep = "Critter object\n"
      rep += "name: " + self.name + "\n"
      return rep
  def talk(self):
    print("Hi. I'm", self.name, "\n")
